{
  "options": {
    "askArray": false,
    "askScale": true,
    "askPosition": false,
    "askAnchor": false,
    "askAngle": false,
    "askObject": false,
    "assetPath": "public/assets/sprites/"
  },
  "variables": {
    "{posX}": 0,
    "{posY}": 0,
    "{ancX}": 0,
    "{ancY}": 0,
    "{scaX}": 1,
    "{scaY}": 1,
    "{ang}": "0+20"
  },
  "sprite":{
    "position": {
      "x": "[posX]-1",
      "y": "[posY]-1"
    },
    "anchor": {
      "x": "[ancX]-1",
      "y": "[ancY]-1"
    },
    "scale": {
      "x": "[{scaX}]-1",
      "y": "[{scaY}]-1"
    },
    "angle": {
      "degree": "[{ang}]-1"
    }
  }
}